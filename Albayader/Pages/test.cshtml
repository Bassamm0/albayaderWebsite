                .photos {
    position: relative;
    display: block;
    box-sizing: border-box;
    width: 700px;
    max-width: 100%;
    margin: 40px auto;
    padding: 10px;
    border: 1px solid #aaa;
    text-align: center;
    font-size: 0;
}
.magnifier {
    position: relative;
    box-sizing: border-box;
    display: inline-block;
    vertical-align: middle;
    width: calc(50% - 8px);
    margin: 4px;
}
.magnifier img {
    position: relative;
    display: inline-block;
    width: 100%;
    height: auto;
}

          </style>

